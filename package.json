{
  "name": "web-marketplaces",
  "version": "1.0.0",
  "description": "Web marketplaces",
  "main": "index.js",
  "scripts": {
    "clean": "rm -f dist/server/public/*.* && npm run create:folders",
    "start": "npm run clean && concurrently -k -p \"[{name}]\" -n \"Client,Server,Node\" -c \"yellow.bold,cyan.bold,green.bold,blue.bold\" \"npm run watch:client\" \"npm run watch:server\" \"npm run server\"",
    "watch:client": "rollup -cw",
    "watch:server": "tsc -w -p tsconfig.server.json",
    "create:folders": "mkdir -p dist/server/public && npm run copy:html",
    "copy:html": "mkdir -p dist/server/views && cp -R src/server/views/* dist/server/views",
    "server": "DEBUG=app nodemon --delay 5 ./dist/server/index.js",
    "dev": "NODE_ENV=development node ./dist/server/index.js",
    "prod": "NODE_ENV=production node ./dist/server/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src --ext .js,.ts,.tsx",
    "build:client": "NODE_ENV=production rollup -c",
    "build:server": "cp ./package*.* ./dist/server && NODE_ENV=production tsc -p tsconfig.server.json",
    "build": "npm run lint && npm run build:client && npm run build:server",
    "build:no-lint": "npm run build:client && npm run build:server",
    "cache:clear": "rm -rf dist/cache/*"
  },
  "repository": "git+https://github.com/SelfKeyFoundation/web-marketplaces.git",
  "author": "Andre Goncalves <andre@andregoncalves.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SelfKeyFoundation/web-marketplaces/issues"
  },
  "homepage": "https://github.com/SelfKeyFoundation/web-marketplaces#readme",
  "dependencies": {
    "airtable": "^0.8.1",
    "bottleneck": "^2.19.5",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.3",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-ejs-layouts": "^2.5.0",
    "flag-icon-css": "^3.5.0",
    "lodash": "^4.17.15",
    "node-fetch": "^2.6.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-router-dom": "^5.2.0",
    "route-cache": "^0.4.4",
    "serve-favicon": "^2.5.0"
  },
  "peerDependencies": {
    "@material-ui/core": "4.9.9",
    "@material-ui/icons": "4.9.1",
    "@material-ui/lab": "4.0.0-alpha.48",
    "@material-ui/styles": "4.9.6"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@rollup/plugin-alias": "^3.1.0",
    "@rollup/plugin-babel": "^5.0.4",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "@rollup/plugin-replace": "^2.3.2",
    "@rollup/plugin-typescript": "^5.0.1",
    "@types/react-dom": "16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "chokidar": "^3.4.0",
    "concurrently": "^5.2.0",
    "eslint": "^7.4.0",
    "eslint-plugin-react": "^7.20.3",
    "nodemon": "^2.0.4",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-url": "^8.0.0",
    "rollup": "^2.21.0",
    "rollup-plugin-postcss": "^3.1.1",
    "rollup-plugin-terser": "^6.1.0",
    "sanitize-html": "^1.27.0",
    "selfkey-ui": "https://github.com/SelfKeyFoundation/selfkey-ui.git#4996672903e1c6793c533059074ad11e8540b67a",
    "tslib": "^2.0.0",
    "typescript": "^3.9.6"
  }
}
